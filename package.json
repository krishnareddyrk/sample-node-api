    "cors": "^2.8.5",
    "express": "^4.16.4",
    "swagger-ui-express": "^4.1.4",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.3.0",
    "rimraf": "^3.0.2"
  }
}
